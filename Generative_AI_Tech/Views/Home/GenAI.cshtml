@section Scripts{
    @if(Model.type == "tools"){
<script>
    $(document).ready(()=>{
        $('#details-card-tools').hide();
    });
    function changeTools(heading,details,img,logo){
        $('#tools-details-heading').html(heading);
        $('#tools-details-dt').html(details);
        $('#tools-logo').attr('src',logo);
        $('#tools-detail-image').attr('src',img);
        $('#details-card-tools').show();
    }
</script>
    }
    else if(Model.type == "organizations"){
<script>
    $(document).ready(()=>{
        $('#details-card-organizations').hide();
    });
    function changeOrganizations(heading,details,img,logo){
        $('#organizations-details-heading').html(heading);
        $('#organizations-details-dt').html(details);
        $('#organizations-logo').attr('src',logo);
        $('#organizations-detail-image').attr('src',img);
        $('#details-card-organizations').show();
    }
</script>
    }
    else if(Model.type == "applications"){
<script>
    $(document).ready(()=>{
        $('#details-card-applications').hide();
    });
    function changeApplications(heading,details,img,logo){
        $('#applications-details-heading').html(heading);
        $('#applications-details-dt').html(details);
        $('#applications-logo').attr('src',logo);
        $('#applications-detail-image').attr('src',img);
        $('#details-card-applications').show();
    }
</script>
    }
}